{
  "name": "ui5-xml-support",
  "displayName": "UI5 XML Support",
  "description": "Syntax highlighting for SAPUI5 XML view.",
  "version": "0.1.1",
  "repository": "github:m8524769/ui5-xml-support",
  "author": "yk <m8524769@gmail.com> (https://m8524769.github.io/)",
  "publisher": "m8524769",
  "license": "MIT",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "compile": "js-yaml syntaxes/xml.tmLanguage.yaml > syntaxes/xml.tmLanguage.json"
  },
  "contributes": {
    "languages": [
      {
        "id": "xml",
        "aliases": [
          "XML",
          "xml"
        ],
        "extensions": [
          ".view.xml",
          ".fragment.xml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "xml",
        "scopeName": "text.xml",
        "path": "./syntaxes/xml.tmLanguage.json",
        "embeddedLanguages": {
          "text.xml": "xml",
          "source.js": "javascript"
        },
        "tokenTypes": {
          "meta.tag string.quoted": "other"
        }
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^3.14.0"
  }
}
